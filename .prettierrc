{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "all",
	"semi": true,
	"printWidth": 80,
	"plugins": [
		"prettier-plugin-organize-imports",
		"prettier-plugin-tailwindcss"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"tailwindStylesheet": "./src/app/globals.css",
	"tailwindFunctions": ["classNames"]
}
